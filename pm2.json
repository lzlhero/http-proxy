[{
  "script": "http-proxy.js",
  "name": "http-proxy",
  "exec_mode": "cluster",
  "instances": 1,
  "log_file": "http-proxy.log",
  "out_file": "/dev/null",
  "error_file": "/dev/null",
  "combine_logs": true,
  "log_date_format" : "YYYY-MM-DD HH:mm:ss",
  "env": {
    "NODE_ENV": "production"
  }
}]
